  "nodes": [
    {
      "file": "node1_base.dart",
      "name": "SliverAppBar基本使用",
      "desc": [
        "【leading】 : 左側コンポーネント   【Widget】",
        "【title】 : 中央コンポーネント   【Widget】",
        "【actions】 : 末尾コンポーネントリスト   【List<Widget>】",
        "【floating】 : フローティングするか   【bool】",
        "【pinned】 : トップに留まるか   【bool】",
        "【snap】 : 半収展するか   【bool】",
        "【bottom】 : 底部コンポーネント   【PreferredSizeWidget】",
        "【expandedHeight】 : 拡張高さ   【double】",
        "【elevation】 : 影の深さ   【double】",
        "【flexibleSpace】 : 拡張スペース   【FlexibleSpaceBar】",
        "【backgroundColor】 : 背景色   【Color】",
        "【controller】 : コントローラー   【ScrollController】",
        "   snapがtrueの場合、floatingはtrueである必要があります"
      ]
    }
  ]
}