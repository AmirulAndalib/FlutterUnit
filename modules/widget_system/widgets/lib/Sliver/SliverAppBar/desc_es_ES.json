        "【leading】 : Componente izquierdo   【Widget】",
        "【title】 : Componente central   【Widget】",
        "【actions】 : Lista de componentes finales   【List<Widget>】",
        "【floating】 : Si flota   【bool】",
        "【pinned】 : Si se queda en la parte superior   【bool】",
        "【snap】 : Si se contrae parcialmente   【bool】",
        "【bottom】 : Componente inferior   【PreferredSizeWidget】",
        "【expandedHeight】 : Altura extendida   【double】",
        "【elevation】 : Profundidad de sombra   【double】",
        "【flexibleSpace】 : Espacio extendido   【FlexibleSpaceBar】",
        "【backgroundColor】 : Color de fondo   【Color】",
        "【controller】 : Controlador   【ScrollController】",
        "   snap debe ser true cuando floating es true"
      ]
    }
